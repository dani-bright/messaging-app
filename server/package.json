{
  "name": "messasing-app-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --list-different \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "coverage": "jest  --coverage --config ./test/jest-e2e.json && yarn test:cov && ts-node merge-coverage.ts --report coverage/e2e/coverage-final.json --report coverage/unit/coverage-final.json"
  },
  "author": "dani",
  "license": "ISC",
  "dependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.8",
